st { register, handleSubmit } = useForm();\n\n  const classes = useStyles();\n\n  const theme = createMuiTheme({\n    palette: {\n      type: \"dark\",\n      primary: {\n        main: \"#c2164f\",\n      },\n    },\n  });\n\n  const imageInput = async (e) => {\n    setIsFetching(true);\n\n    // uploads the file to storage and gives the url of it to setPostImage\n    const file = e.target.files[0];\n\n    const storageRef = db.storage().ref();\n\n    const fileRef = storageRef.child(file.name);\n\n    await fileRef.put(file);\n\n    fileRef.getDownloadURL().then((url) => {\n      setPostImage(url);\n      console.log(\"uloaded\");\n      setIsFetching(false);\n    });\n  };\n\n  const handleEditProfile = () => {\n    const fileInput = document.getElementById(\"imageInput\");\n\n    fileInput.click();\n  };\n\n  const handleLogOut = () => {\n    console.log(\"Logged out!\");\n\n    props.signOutThunk();\n    props.withSignOutThunk();\n\n    history.push(\"/signup\");\n  };\n\n  //--------- SAVE INFO ---------\n  const onSubmit = (data) => {\n    setEditProfile(true);\n\n    props.setUserInfoThunk(\n      data.fullname,\n      data.username,\n      data.status,\n      data.location,\n      postImage,\n      props.currentUserId\n    );\n    setIsDisabled(!isDisabled);\n  };\n\n  const handleEdit = () => {\n    setEditProfile(false);\n\n    setIsDisabled(!isDisabled);\n  };\n\n  return (\n    <FeedWrapper>\n      <StickyTop header={\"Profile\"} mobile={props.mobile}/>\n      <Button\n        onClick={handleLogOut}\n        className=\"profileContainer__logout\"\n        variant=\"outlined\"\n        color=\"secondary\"\n        type=\"submit\"\n        variant=\"outlined\"\n      >\n      Sign out\n      </Button>\n      <div className={!isDisabled ? \"profile__Enabled\" : \"profile__Disabled\"}>\n        <div className=\"profile__UploadWrapper\">\n          <div className=\"profile__Wrapper\">\n            <div className=\"profile__Picture\">\n              <div class